<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="../data/image/icon.jpg">
    <title>Project Detail</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css"
        integrity="sha512-Kc323vGBEqzTmouAECnVceyQqyqdsSiqLQISBL29aUW4U/M7pSPA/gEUZQqv1cwx4OnYxTxve5UMg5GT6L4JJg=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />

    <link rel="stylesheet" href="../data/css/nav.css">

</head>

<body>
    <!-- Start Navbar -->

    <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
            <a class="navbar-brand ms-2" href="/profile">
                <img src="../data/image/logo.png" alt="" style="width: 50px;">
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse hovers" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item fs-5 ms-2">
                        <a class="nav-link hovers hover active" href="/"><i class="fa-solid fa-arrow-left"></i> Back</a>
                    </li>
                </ul>
                <a class="btn btn-dark me-3 fw-bold contact px-3" href="/contact">Contact Me</a>
            </div>
        </div>
    </nav>

    <!-- End Navbar -->
    <!-- Project Detail Card -->

    <h1 class="text-center fw-semibold my-5">{{data.project}}</h1>
    <div class="">
        <div class="m-auto bg-transparent" style="max-width: 1100px;">
            <div class="row g-0">
                <div class="col-md-4" style="width: 570px;">
                    <img src="../data/image/cyborg.jpg" class="img-fluid rounded-start" style="width: 1000px;"
                        alt="...">
                </div>
                <div class="col-md-5 ms-5 row align-items-center">
                    <div class="card-body mb-3">
                        <h5 class="card-title">Duration</h5>
                        <p class="card-text"><i class="fa-solid fa-calendar-days fa-beat mt-3 me-2"
                                style="font-size: 30px;"></i> {{data.start}} | {{data.end}}</p>
                        <p class="card-text"><i class="fa-solid fa-clock fa-beat me-2" style="font-size: 30px;"></i> {{data.duration}}
                        </p>
                    </div>
                    <div class="card-body">
                        <h5 class="card-title">Technologies</h5>
                        <div class="grid gap-3 fs-2">
                            {{{data.swift}}}
                            {{{data.ruby}}}
                            {{{data.python}}}
                            {{{data.javascript}}}
                        </div>
                    </div>
                </div>
            </div>
            <div>
                <p class="card-text my-5" style="text-align: justify;">{{data.description}}</p>
            </div>
        </div>
    </div>

    <!-- Project Detail Card -->


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
</body>

</html>