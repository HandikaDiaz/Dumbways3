    <!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="../data/image/icon.jpg">
    <title>Project</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

    <link rel="stylesheet" href="../data/css/nav.css">
    <link rel="stylesheet" href="../data/css/project.css">
</head>

<body>
    <!-- Start Navbar -->

    <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
            <a class="navbar-brand ms-2" href="/profile">
                <img src="../data/image/logo.png" alt="" style="width: 50px;">
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse hovers" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item fs-5 ms-2">
                        <a class="nav-link hovers" href="/">Home</a>
                    </li>
                    <li class="nav-item fs-5 ms-2">
                        <a class="nav-link hovers active" href="/project">Project</a>
                    </li>
                    <li class="nav-item fs-5 ms-2">
                        <a class="nav-link hovers" href="/testimonial">Testimonial</a>
                    </li>
                </ul>
                <a class="btn btn-dark me-3 fw-bold contact px-3" href="/contact">Contact Me</a>
            </div>
        </div>
    </nav>

    <!-- End Navbar -->
    <!-- Start Project Form -->

    <form action="/" method="POST" class="form pb-5 mb-5" id="form" autocomplete="off">
        <h1 class="text-center fw-semibold">Add Project</h1>
        <div class="w-50 m-auto fs-5 fw-bold">
            <div class="mb-4 mt-5">
                <label for="inputProject" class="form-label">Project Name</label>
                <input type="text" class="form-control" id="inputProject" name="project">
            </div>
            <div class="row mb-4">
                <div class="col">
                    <label for="inputStartDate">Start Date</label>
                    <input type="date" class="form-control" id="inputStartDate" name="startDate"> 
                </div>
                <div class="col">
                    <label for="inputEndDate">End Date</label>
                    <input type="date" class="form-control" id="inputEndDate" name="endDate">
                </div>
            </div>
            <div class="mb-4">
                <label for="inputDescription">Description</label>
                <textarea class="form-control" id="inputDescription" style="height: 100px"
                    name="description"></textarea>
            </div>
            <div class="mb-4">
                <label for="">Technologies</label>
                <div class="flex-check">
                    <div class="flex-box">
                        <label for="swift"><input type="checkbox" class="me-2" id="swift" name="swift">Swift</label>
                        <label for="ruby"><input type="checkbox" class="me-2" id="ruby" name="ruby">Ruby</label>
                    </div>
                    <div class="flex-box">
                        <label for="phyton"><input type="checkbox" class="me-2" id="phyton" name="python">Python</label>
                        <label for="javascript"><input type="checkbox" class="me-2" id="javascript" name="javascript">Javascript</label>
                    </div>
                </div>
            </div>
            <div class="mb-3">
                <label for="inputImage" class="form-label">Upload Image</label>
                <input class="form-control" type="file" id="inputImage">
            </div>
            <button class="btn btn-dark float-end fw-bold my-3 px-4 rounded-pill submit">Submit</button>
        </div>
    </form>

    <!-- End Project Form -->
    
    <!-- Start Script -->

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
    {{!--
    <script src="../data/js/project.js"></script> --}}

    <!-- End Script -->
</body>

</html>